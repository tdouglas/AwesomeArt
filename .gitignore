  <%= render '..application/stop' %>
 << artwork %>
  <% end %>

    <% array = [] %>
  <% artwork = {} %>

  <% @images_array.each do |info| %>
    <% t = info[:title] %></br>
    <% a = info[:artist] %></br>
    <% m = info[:medium] %></br>
    <% c = info[:collection] %></br>
    <% artwork[:title] = t %>
    <% artwork[:artist] = a %>
    <% artwork[:medium] = m %>
    <% artwork[:collection] = c %>
    <% array << artwork %>
    <%= array.class %>
  <% end %>

  <% @images_array.each do |info| %>
    Title: <%= info[:title] %></br>
    Artist: <%= info[:artist] %></br>
    Medium: <%= info[:medium] %></br>
    Collection: <%= info[:collection] %></br>
  <% end %>


